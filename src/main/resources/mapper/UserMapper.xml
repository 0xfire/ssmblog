<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zed.dao.UserDao">

    <insert id="register" parameterType="com.zed.model.User" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO user_table
        (name,password,gender,hobby,regDate,userInfo,avatar,homeAddress,workAddress)
        VALUES
        (#{name},#{password},#{gender},#{hobby},#{regDate},#{userInfo},#{avatar},#{homeAddress},#{workAddress})
    </insert>
</mapper>